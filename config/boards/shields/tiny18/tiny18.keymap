#define AL1 0
#define AL2 1

/ { 
    keymap {
        compatible = "zmk,keymap";

        alpha1 { 
            bindings = <
                        &kp S    &kp R   &kp T           &kp N   &kp E   &kp A
                &kp I   &kp L    &kp G   &kp D           &kp H   &kp U   &kp O &kp C
                                &trans &trans       &lt AL2 SPACE &trans
            >;
        };

        alpha2 {
            bindings = <
                        &kp J    &kp P   &kp K           &kp B   &kp DOT  &kp X
                &kp Q   &kp V    &kp W   &kp M           &kp F   &kp COMM &kp Z &kp Y
                                  &kp BSPC &kp SPC   &trans    &kp TAB
            >;
        };
    };
};
